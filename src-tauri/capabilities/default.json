{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-open",
    "fs:read-dirs",
    "fs:read-files",
    "fs:allow-audio-read",
    "fs:allow-audio-read-recursive",
    "fs:allow-download-read",
    "fs:allow-download-read-recursive",
    "opener:default",
    "opener:default"
  ]
}
